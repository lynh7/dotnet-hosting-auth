{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5004"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/v1.0/book",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 9223
        }
      ],
      "UpstreamPathTemplate": "/api/book",
      "UpstreamHttpMethod": [ "OPTIONS", "POST", "GET", "PUT", "DELETE" ]
    },
    {
      "DownstreamPathTemplate": "/v1.0/Auth/login/{type}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7081
        }
      ],
      "UpstreamPathTemplate": "/api/login/{type}",
      "UpstreamHttpMethod": [ "OPTIONS", "POST", "GET", "PUT", "DELETE" ]
    }
  ]
}